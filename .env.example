# PlayCamp Example Server Configuration
# Copy this file to .env and fill in your values

# Required: Server API Key (get from PlayCamp dashboard)
SERVER_API_KEY=ak_server_your_key_id:your_secret

# Required: Webhook secret (for signature verification)
WEBHOOK_SECRET=your_webhook_secret_hex

# SDK Environment: 'sandbox' or 'live' (default: live)
SDK_ENVIRONMENT=sandbox

# Or use custom API URL (overrides SDK_ENVIRONMENT)
# SDK_API_URL=http://localhost:3003

# Enable SDK debug logging
SDK_DEBUG=true

# Server port (default: 4000)
PORT=4000
